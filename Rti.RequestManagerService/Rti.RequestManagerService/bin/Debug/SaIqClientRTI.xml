<?xml version="1.0" encoding="ISO-8859-1" ?>
<!-- Production Area -->
<iqServerConfig name="rti QA" version="1.0" info="rti QA">
  <serverGroupConfig>
    <defaultGroup name="default" group="default" active="true">

      <clientParams>
        <param name="xmlBaseDir" value="stream"/>
        <param name="commTimeout" value="300000"/>
        <param name="connectionTimeout" value="300000"/>
        <param name="logDir" value="."/>
        <param name="progress.logLevel" value="10"/>
        <param name="java.logLevel" value="WARNING"/>
        <param name="java.connectionClass" value="com.solvepoint.iqrpc.client.SocketConnection"/>
      </clientParams>
    </defaultGroup>
  </serverGroupConfig>

  <servers>
    <!-- "server" is the DTD equivalent of "group" -->
    <server name="master" group="default" active="true">
      <clientParams>
        <param name="server.url.list" value="rtitsta.rti.com:20135"/>
        <param name="xmlBaseDir" value="stream"/>
      </clientParams>
    </server>
    <server name="mpi" group="main" active="true">
      <clientParams>
        <param name="server.url.list" value="rtitsta.rti.com:20192"/>
        <param name="xmlBaseDir" value="stream"/>
      </clientParams>
    </server>
    <server name="longrpc" group="main" active="true">
      <clientParams>
        <param name="server.url.list" value="rtitsta.rti.com:20600"/>
        <param name="xmlBaseDir" value="stream"/>
        <param name="commTimeout" value="0"/>
      </clientParams>
    </server>
  </servers>

</iqServerConfig>

